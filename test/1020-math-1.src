
func print(n i32) {
	_hexout_(n);
}

func test(v2 i32, v5 i32, v31 i32, v128000 i32) {
	// quick check hex and bin consts
	print(0xFFD23137);
	print(0b10001000110100000101000010100001);
	print(v2);
	print(v128000 / 1000);
	print(1 << v5);
	print(~v31);
	print(v31 % 17);
	print(v128000 * v31);
	print(-v128000);
}

func start() i32 {
	test(2, 5, 31, 128000);
	return 0;
}
